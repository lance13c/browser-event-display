{
  "manifest_version": 2,

  "name": "Event Data",
  "description": "An chrome extension to test gathering content script window event data",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pixi.min.js", "app.js"]
    }
  ]
}