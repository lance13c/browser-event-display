{
  "manifest_version": 2,

  "name": "Event Data",
  "description": "An chrome extension to test gathering content script window event data",
  "version": "1.0",

  "browser_action": {
    "default_icon": "app/assets/image/icon.png",
    "default_popup": "app/view/index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": ["app/style/style.css", "node_modules/materialize-css/bin/materialize.css"],
    "js": ["node_modules/jquery/dist/jquery.min.js", "node_modules/materialize-css/bin/materialize.js", "app/assets/js/pixi.min.js", "app/js/app.js"]
  }
  ]
}